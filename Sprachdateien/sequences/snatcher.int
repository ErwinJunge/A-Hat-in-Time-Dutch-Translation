[manor]
intro0 = [shake2]Hey hey now! No reason to throw a fist at your old buddy, [wave]ha ha ha ha[/wave]![/shake2]
intro1 = [shake2]What are you doing up here? Don't you know she'll go mental if she catches you?[/shake2]
intro2 = [shake2]What, me? Just looking for some dirt on my old friend [name]the Moonjumper[/name][/shake2].
intro3 = [shake2]Poor fellow, doesn't even know he used to live here! Eeeeevery time I bring him something from this place, he almost wets his bed in fear, [wave]HA HA HA!![/wave][/shake2]
intro4 = [shake2][name]Subcon Forest[/name] isn't big enough for the two of us, you know. And now you, a little girl, are trying to enter the fight for the forest as well? My my, that won't do.[/shake2]
intro5 = [shake2]You need to go up, or else she'll come up here and find you, right? I guess it would be a terrible misfortune if [hint]the key disappeared[/hint], [wave]ha ha ha[/wave]![/shake2]

chase0 = [shake2][wave]Ha ha ha![/wave] It's mine![/shake2]
chase1 = [shake2]What are you gonna do? Don't you know, you can't [hint]hurt a shadow[/hint]![/shake2]

givekey0 = [shake2]Oh, du willst den Schlüssel? Kannst du gerne haben![/shake2]
givekey1 = [shake2]Ich brauche den Schlüssel eh nicht... du aber auch nicht wenn sie dich fängt! [wave]HA HA HA![/wave][/shake2]

enter_manor_hascontract0 = [shake2]Hey Kumpel! Erinnere dich an unseren Vertrag! Keine Hüte im Hause![/shake2]
enter_manor_hascontract1 = [shake2]D ist... zeug... im Haus mit dem zu nicht rumspielen solltest! Denk an deine vertraglichen Verflichtungen![/shake2]

[subconforest]
scooter_intro0 = [shake2]GUUUUUUUTEEEEEEN MOOOORGEEEN! Bist du bereit für deine vertraglichen Vorschriften?[/shake2]
scooter_intro1 = [shake2]Ich bin nicht Herzlos! Hin und wieder klaue ich Briefe aus Briefkästen und bringe sie meinen Untertanen. So denken sie dann das sich jemand um sie kümmert![/shake2]
scooter_intro2 = [shake2]Aber, unser letzter Postbote ist...naja.. [wave]*poof*[/wave]. Deswegen musst du es machen![/shake2]
scooter_intro3 = [shake2]Ich spendiere dir sogar ein "Geschenk" für deine Aufgabe. Allerdings brauchst du einen bestimmten Hut dafür, Ich hoffe du hast ihn! [wave]Ha ha ha![/wave][/shake2]

scooter_onenter0 = [shake2]Dieser [icon:badge][name]Anstecker[/name] ist nur geliehen. Ich brauche ihn zurück bevor du abhaust! Denk daran das Abzeichen auf deinen [icon:sprinthat][name]Sprint Hut[/name]zu packen, sonst funktioniert es nicht! [wave][scream]HA HA HA HA HA!![/scream][/wave][/shake2]
scooter_onenter1 = [shake2]Alle Untertanen die auf ihre Post warten haben [hint]ein Symbol über ihnen[/hint]. Wenn du Fertig bist, komm zu mir zurück und Stempel dich aus. Verstanden?[/shake2]

[contract_initial]
intro0 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake]
intro1 = [shake][big][scream][wave]NAAAAAAAAAAAAAAARRRR!![/wave][/scream][/big][/shake]
intro2 = [shake2]Du hast es vermasselt! Komplett vermasselt! Keiner betritt meinen Wald und verlässt ihn in einem Stück![/shake2]
intro3 = [shake2]Ich sag dir was, warte eine Minute! Du darfst Leben! Du hast richtig gehört, du Glückspilz![/shake2]
intro4 = [shake2]Normalerweise würde ich deine Seele essen und deinen Körper wegwerfen, aber heute nicht! Du hast die Lotterie gewonnen, denn ich benötige einen hirnlosen Sklaven![/shake2]
intro5 = [shake2]Du hast richtig gehört! Der andere ist gestorben. Sein Kopf ist geplatzt! Jetzt brauche ich einen neuen Laufburschen![/shake2]
contract_appear0 = [shake2]Hör gut zu. Ich habe ein Stück Papier hier, kümmer dich nicht um das Kleingedruckte! Du musst nur hier unten Unterschreiben![/shake2]

contract_disagree_retry = Lass mich nachdenken...
contract_disagree_decline = Ich unterschreibe nicht!

contract_disagree0 = [shake2]Komm schon, mach das nicht schwierig! Du hast nicht wirklich eine Wahl! Unterschreib das Papier![/shake2]
contract_disagree1 = [shake2]Ich glaube nicht ich hab mich klar genug ausgedrückt: WAS DU DENKST INTERESSIERT MICH NICHT. Schreib deinen Namen auf den Vertrag oder ich werde deinen Leblosen Körper dafür nutzen![/shake2]
contract_disagree2 = [shake2]LETZTE CHANCE! Unterschreib das Papier![/shake2]
contract_disagree3 = [shake2]Zu Schade! Ich gab dir eine möglichkeit eine schlechte Situation wieder hinzubekommen, aber du hast es vermasselt! Deine einzige überlebens chance direkt aus dem Fenster geworfen![/shake2]
contract_disagree4 = [shake2]Tschüss! Ich mein, tschüss zu deinem Selbstbewusstsein, und Hallo an deinen Leblosen Körper![/shake2]

contract_agree0 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake]
contract_agree1 = [shake2]Das schließt den Deal ab! Wir sind im Geschäft! Ich nehm das hier nur kurz...[/shake2]

contract_soul = Du fühlst dich außerordentlich leer von innen.

contract_agree2 = [shake2]Whoops! Deine Seele gehört jetzt mir![/shake2]
contract_agree3 = [shake2]Aber keine Angst! Wenn du deine Aufgaben schnell erledigst, bekommst du deine Seele wieder![/shake2]
contract_agree4 = [shake2]Und vergiss bloß nicht unseren Vertrag! Ich habe ihn in dein [button:start] Menü getan, damit du ihn nie aus den Augen verlierst![/shake2]
contract_agree5 = [shake2]Auf Wiedersehen! Sei Vorsichtig! Versuch nicht zu sterben![/shake2]

enter_village0 = [shake][big][scream][wave]HALLÖÖÖÖÖÖCHEEEEEEN![/wave][/scream][/big][/shake][br][shake2]Du hast mich doch nicht vergessen, oder?! Wehe wenn doch![/shake2]
enter_village1 = [shake2]Wie gehts? Arbeitest du gerade an deinen Vertraglichen Verpflichtungen? Hoffentlich! Denk an unseren Deal![/shake2]
enter_village2 = [shake2]Zieh dir das rein. Diese nervigen [name]Feuer Geister[/name] die dort rumsitzen, mitten in meinem Dorf.[/shake2]
enter_village3 = [shake2]Ihr kleiner Tanz erschafft diese sehr nervigen Barrieren die mir den Zugang zu meinem Wald abblocken. Fühlt sich fast so an als würden sie mich los werden wollen. Wie süß.[/shake2]
enter_village4 = [shake2]Werd' sie los. Keine Ahnung wie du es machst, hauptsache sie leiden dafür das sie meinen Wald betreten haben! [wave]HEHEHEHE![/wave][/shake2]
enter_village5 = [shake2]Jedenfalls, zurück an die Arbeit, Sklave![br]Das hier ist kein Urlaub, und ich werde ziemlich ungeduldig![/shake2]

[breakice_contract]
breakice0 = [shake][big][scream][wave]GUT GEMACHT![/wave][/scream][/big][/shake][br][shake2]Bitte sag mir sie haben gelitten. Du hast sie getötet, oder? Ich wette, du hast sie erdrosselt! Gutes Mädchen! [scream][wave]Ha ha ha ha![/wave][/scream][/shake2]
breakice1 = [shake2]Noch sind wir aber nicht fertig! Es sind noch mehr als genug Plagegeister in meinem Wald, und du musst dich um sie kümmern.[/shake2]
breakice2 = [shake2]Das nächste auf deiner Liste: Säubere den Subcon Brunnen. Das ist richtig, du bist jetzt ein Klempner! Glückwunsch zu deiner Beförderung![/shake2]
breakice3 = [shake2]Ich habe auch noch...andere Arbeit für dich die ich nicht selbst machen kann. Aber hey, solange ich dich brauche bleibst du am Leben. Was ein Deal, oder?[/shake2]

breakice4 = [shake2]Genug geredet! Du siehst aus als könntest du mehrere Aufgaben ausführen! Wie wärs mit einem weiteren Vertrag?[/shake2]
breakice5 = [shake2]Nimm einen, aber nur einen! Ist es nicht spannend sein Leben ?[/shake2]

breakice6 = [shake2]Gute Wahl! Hoffentlich schaffst du den Vertrag auch![/shake2]
breakice7 = [shake2]Du bist auf der Suche nach [icon:timepiece][timepiece]Zeitstücken[/timepiece], oder? Ich habe eins gefunden während ich jemanden erschreckt habe![/shake2]
breakice8 = [shake2]Du darfst es behalten, aber das ist das einzige mal wo ich dich Belohnen werde! Denn im Vertrag stand nie was von einer Vergütung! [wave][scream]HA HA HA HA HA![/scream][/wave][/shake2]

[contract_generic_trap]
trapA0 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake][br][shake2]NAAAAAArrr... oh, du bist es..[/shake2]
trapA1 = [shake2]Ich habe gehofft das jemand anderes in meine Falle tritt. Ich habe schon deine Seele genommen und alles. Hast du überhaubt deinen anderen Vertrag schon fertig?[/shake2]
trapA2 = [shake2]Nimm's nicht Persönlich, kleine. Aber bleib von meinen Fallen fern. Ich kann nicht so oft Schreien wie's mir lieb ist. Du nimmst die Erfahrung weg von anderen![/shake2]
trapA3 = [shake2]Aber, da du ja eh in meine Falle reingelaufen bist, verlängern wir einfach mal deine Verträge![/shake2]
trapA4 = [shake2][small]*hust*[/small][/shake2][br] [br][shake][big][scream][wave]NAAAAAAAAAAAAAAAAAARRRR!![/wave][/scream][/big][/shake]
trapA5 = [shake2]Du hast meinen Wald betreten, und nun musst du dafür Bezahlen![/shake2]
trapA6 = [shake2]Such dir was aus! Welche Last willst du für den rest deines Lebens mit dir rumschleppen?![/shake2]

trapA7 = [shake2]Keine schlechte Wahl! Das sollte ein Kinderspiel sein! Du lässt aber alle ganzen schweren Aufträger für die Neulinge. Was ziemlich unhöflich ist wenn man darüber nachdenkt.[/shake2]
trapA8 = [shake2]Was auch immer! Ich bin weg! Bis zum nächsten mal![/shake2]

