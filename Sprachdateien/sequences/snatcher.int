turned_blue2 = [shake2]Das kann nicht zählen, oder? Das kann doch wahrlich nicht zählen.[/shake2]
turned_blue3 = [shake2]Jetzt reicht es. Zeit zu [scream][wave]STERBEN[wait][/wave][/scream].[/shake2]

[death]
death0 = [shake2]*huff*[waitlong] *huff*[waitlong] Du bist so eine respektlose Göre...[/shake2]
death1 = [shake2]Ich fühle mich... so schwach...[waitlong] *huff*[/shake2]
death2 = [shake2]Bitte...[waitlong] *huff*[waitlong] habe Gnade...[/shake2]
death3 = [shake2][waitlong][wait]Bitte... [br] [waitlong][waitlong][br][big]SETZ' DICH DOCH[/big][/shake2][waitlong][waitlong]
death4 = [shake2]All diese [icon:timepiece][timepiece]Zeitstücke[/timepiece] sind in MEINEM Wald gelandet. Sie sind MEIN Eigentum.[/shake2]
death5 = [shake2]So, wie wär's mit einem Deal, junge Dame? Du verschwindest aus meinem Wald, und wir sehen dich hier nie wieder. Wie hört sich das an?[/shake2]
death6 = [shake2]Nicht gut? Was für 'n Jammer! Du hast hier nichts zu sagen.[/shake2]
death7 = [shake2]Hier, ich mach' es dir sogar einfach![/shake2]
death8 = [shake2]Alles was du machen musst, ist zu unterschreiben![/shake2]

sign0 = [shake2]...Du weißt schon, dass Verträge so nicht funktionieren, oder?[/shake2]
sign1 = [shake2]Ich geb' dir 'ne Chance abzuhauen, Kleine! Keine Tricks dieses Mal! [/shake2]
sign2 = [shake2]Was wenn ich dir deine Seele zurück gebe? Haust du dann ab?[/shake2]
sign3 = [shake2]Ja? Nein? Hallo? Ist da jemand? Warum sagst du nichts?[/shake2]
sign4 = [shake2].......[/shake2]
