[subconforest]
scooter_intro0 = [shake2]GUUUUUUUTEEEEEEN MOOOORGEEEN! Bist du bereit für deine vertraglichen Verpflichtungen?[/shake2]
scooter_intro1 = [shake2]Ich bin nicht herzlos! Hin und wieder klaue ich Briefe aus Briefkästen und bringe sie meinen Untertanen. Damit denken sie, dass sich jemand um sie kümmert![/shake2]
scooter_intro2 = [shake2]Aber unser letzter Postbote ging... naja... [wave]*poof*[/wave]. Deswegen musst du es machen![/shake2]
scooter_intro3 = [shake2]Ich spendiere dir sogar ein "Geschenk" für deine Aufgabe. Allerdings brauchst du einen bestimmten Hut dafür. Ich hoffe du hast ihn! [wave]Ha ha ha![/wave][/shake2]

scooter_onenter0 = [shake2]Dieser [icon:badge][name]Anstecker[/name] ist nur geliehen. Ich brauche ihn zurück bevor du abhaust! Denk daran ihn auf deinen [icon:sprinthat][name]Sprinthut[/name] zu packen, sonst tut er überhaupt nichts! [wave][scream]HA HA HA HA HA!![/scream][/wave][/shake2]
scooter_onenter1 = [shake2]Alle Untertanen die auf ihre Post warten haben [hint]ein Symbol über ihnen[/hint]. Wenn du fertig bist, kommst du zu mir zurück und stempelst dich aus. Verstanden?[/shake2]

[contract_initial]
intro0 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake]
intro1 = [shake][big][scream][wave]NAAAAAAAAAAAAAAARRRR!![/wave][/scream][/big][/shake]
intro2 = [shake2]Du hast es vermasselt! Komplett vermasselt! Keiner betritt meinen Wald und verlässt ihn in einem Stück![/shake2]
intro3 = [shake2]Ich sag dir was, warte eine Minute! Du darfst leben! Du hast richtig gehört, du Glückspilz![/shake2]
intro4 = [shake2]Normalerweise würde ich deine Seele essen und deinen Körper wegwerfen, aber heute nicht! Du hast die Lotterie gewonnen, denn ich benötige einen hirnlosen Sklaven![/shake2]
intro5 = [shake2]Du hast richtig gehört! Der andere ist gestorben. Sein Kopf ist geplatzt! Jetzt brauche ich einen neuen Laufburschen![/shake2]
contract_appear0 = [shake2]Hör' gut zu. Ich habe hier ein Stück Papier, kümmere dich nicht um das Kleingedruckte! Du musst nur hier unten unterschreiben![/shake2]

contract_disagree_retry = Gutes Argument.
contract_disagree_decline = Ich unterschreibe nicht!

contract_disagree0 = [shake2]Komm schon, mach das nicht schwierig! Du hast nicht wirklich eine Wahl! Unterschreib das Papier![/shake2]
contract_disagree1 = [shake2]Ich glaube nicht ich hab mich klar genug ausgedrückt: WAS DU DENKST INTERESSIERT MICH NICHT. Schreib deinen Namen auf den Vertrag oder ich werde deinen leblosen Körper dafür nutzen![/shake2]
contract_disagree2 = [shake2]LETZTE CHANCE! Unterschreibe das Papier![/shake2]
contract_disagree3 = [shake2]Schade! Ich gab dir eine Möglichkeit eine schlechte Situation wieder hinzubekommen, aber du hast es vermasselt! Deine einzige[br]Überlebenschance direkt aus dem Fenster geworfen![/shake2]
contract_disagree4 = [shake2]Tschüss! Ich mein', "Tschüss" zu deinem Selbstbewusstsein und "Hallo" zu deinem leblosen Körper![/shake2]

contract_agree0 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake]
contract_agree1 = [shake2]Das schließt den Deal ab! Wir sind im Geschäft! Ich nehm' das hier nur kurz...[/shake2]

contract_soul = Du fühlst dich außerordentlich leer von innen an.

contract_agree2 = [shake2]Uups! Deine Seele gehört jetzt mir![/shake2]
contract_agree3 = [shake2]Aber keine Angst! Wenn du deine Aufgaben schnell erledigst, bekommst du deine Seele wieder![/shake2]
contract_agree4 = [shake2]Und vergiss bloß nicht unseren Vertrag! Ich habe ihn in dein [button:start]-Menü getan, damit du ihn nie aus den Augen verlierst![/shake2]
contract_agree5 = [shake2]Auf Wiedersehen! Sei vorsichtig! Versuch' nicht zu sterben![/shake2]

enter_village0 = [shake][big][scream][wave]HALLÖÖÖÖÖÖCHEEEEEEN![/wave][/scream][/big][/shake][br][shake2]Du hast mich doch nicht vergessen, oder?! Sag' mir nicht, du hast mich vergessen![/shake2]
enter_village1 = [shake2]Wie geht's? Arbeitest du gerade an deinen vertraglichen Verpflichtungen? Hoffentlich! Denk' an unseren Deal![/shake2]
enter_village2 = [shake2]Zieh' dir das rein. Diese nervigen [name]Feuergeister[/name] sitzen mitten in meinem Dorf.[/shake2]
enter_village3 = [shake2]Ihr kleiner Tanz erschafft diese unglaublich nervigen Barrieren, die mir den Zugang zu meinem Wald blockieren. Es ist fast so, als würden sie versuchen mir den Garaus zu machen. Wie niedlich.[/shake2]
enter_village4 = [shake2]Werd' sie los. Mich kümmert es nicht wie du es machst. Hauptsache ist, dass sie leiden müssen! [wave]HEHEHEHE![/wave][/shake2]
enter_village5 = [shake2]Jedenfalls, zurück zur Arbeit, Soldat![br]Das hier ist kein Urlaub, und ich werde so langsam ungeduldig![/shake2]

[breakice_contract]
breakice0 = [shake][big][scream][wave]GUT GEMACHT![/wave][/scream][/big][/shake][br][shake2]Bitte sag mir, dass sie gelitten haben. Du hast sie getötet, oder? Ich wette, du hast sie erdrosselt! Gutes Mädchen! [scream][wave]Ha ha ha ha![/wave][/scream][/shake2]
breakice1 = [shake2]Glaub aber nicht, dass wir schon fertig sind! Es sind noch mehr als genug Plagegeister in meinem Wald, und du musst dich um sie kümmern.[/shake2]
breakice2 = [shake2]Das nächste auf deiner Liste: Säubere den Subcon-Brunnen. Das ist richtig, du bist jetzt ein Klempner! Glückwunsch zu deiner Beförderung![/shake2]
breakice3 = [shake2]Ich habe auch noch... andere Arbeit für dich, die ich nicht selbst machen kann. Aber hey, solange ich dich brauche bleibst du am Leben. Was für ‘n Deal, oder?[/shake2]

breakice4 = [shake2]Genug geredet! Du siehst aus als könntest du mehrere Dinge gleichzeitig bewältigen! Wie wär's mit einem weiteren Vertrag?[/shake2]
breakice5 = [shake2]Nimm' einen, aber nur einen! Ist es nicht spannend seinem Leben zu unterschreiben?[/shake2]

breakice6 = [shake2]Gute Wahl! Hoffentlich schaffst du den Vertrag auch![/shake2]
breakice7 = [shake2]Du bist auf der Suche nach [icon:timepiece][timepiece]Zeitstücken[/timepiece], oder? Ich habe eins gefunden während ich eine arme Seele gejagt habe![/shake2]
breakice8 = [shake2]Du darfst es behalten, aber das ist das einzige Mal das ich dich belohnen werde! Denn im Vertrag stand nie was von einer Vergütung! [wave][scream]HA HA HA HA HA![/scream][/wave][/shake2]

[contract_generic_trap]
trapA0 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake][br][shake2]NAAAAAArrr... oh, du bist's..[/shake2]
trapA1 = [shake2]Ich hatte eigentlich gehofft, dass ein neues Opfer in meine Falle tritt. Ich habe schon deine Seele genommen und alles. Hast du überhaupt deinen anderen Vertrag schon abgeschlossen?[/shake2]
trapA2 = [shake2]Nimm's nicht persönlich, Kleine. Aber bleib' von meinen Fallen fern. Ich kann nicht so oft schreien wie's mir lieb ist. Du nimmst anderen Leuten das Erlebnis![/shake2]
trapA3 = [shake2]Aber, da du ja eh in meine Falle gelaufen bist, kann ich auch gleich unsere Geschäftsbezihungen verlängern![/shake2]
trapA4 = [shake2][small]*hust*[/small][/shake2][br] [br][shake][big][scream][wave]NAAAAAAAAAAAAAAAAAARRRR!![/wave][/scream][/big][/shake]
trapA5 = [shake2]Du hast meinen Wald betreten, und nun musst du dafür bezahlen![/shake2]
trapA6 = [shake2]Such' dir was aus! Welche Last willst du für den Rest deines Lebens tragen?![/shake2]

trapA7 = [shake2]Keine schlechte Wahl! Das wird' ein Kinderspiel sein! Du lässt aber alle schweren Aufträge für die Neulinge. Was ziemlich gemein ist wenn man darüber nachdenkt.[/shake2]
trapA8 = [shake2]Was auch immer! Ich bin weg! Bis zum nächsten Mal![/shake2]

[home]
home_question0 = [shake2]Wie geht's, Kleine? Du weißt nicht, wo du hingehen musst?[/shake2]
home_question0_whereto = Wo muss ich hin?
home_question0_contract = Vertrag abschließen

home_intro0 = [shake2]Warum störst du mich? Hast du nicht den Vertrag gelesen, bevor du ihn unterschrieben hast?[/shake2]
home_intro1 = [shake2]Du bekommst nichts von mir bis du deine Aufgaben erledigt hast! Hau ab![/shake2]

nosoul0 = [shake2]Du dachtest doch wohl nicht, dass du deine Seele nach dem ersten Vertrag zurückbekommst, oder?[/shake2]
nosoul1 = [shake2]Na, [shake][big][scream]ZU SCHADE![/scream][/big][/shake][/shake2][br][shake2]Du bist mein neuer Sklave, und wir sind noch nicht fertig! Schließe deine Aufgaben ab, dann reden wir![/shake2]

home_businessconcluded = [shake2]Kind, wir sind fertig. Ich gebe dir keine weiteren Aufträge. Hau ab![/shake2]

wheretogo_subconwell0 = [shake2]Du solltest doch den Brunnen sauber machen! Er ist tief im Sumpf, aber groß genug sodass etwas Kleines wie du ihn nicht verfehlen kannst. Versuch bitte nicht zu fallen und mit dem Kopf am Boden aufzuschlagen... [wave][scream]HA HA HA HA HA HA![/scream][/wave][/shake2]

postfinale0 = [shake2]Du schon wieder? Ich war mir ziemlich sicher, dass ich mich beim letzten Mal klar ausgedrückt habe, Kleine.[/shake2]
postfinale1 = [shake2]Wir sind fertig. Komplett. Unsere Geschäftsbeziehung ist vorbei. Ich verschwende keine Vorräte mehr an dir![/shake2]
postfinale2 = [shake2][wave][scream][big]Jetzt verschwinde aus meinem Wald![/big][/scream][/wave][/shake2]

[bossbattle]
intro0 = [shake2]Warte kurz, Kleine. Erinnerst du dich daran, wie der alte Auftragnehmer seinen Kopf verloren hat?[/shake2]
intro1 = [shake2]Das war kein Unfall. Er fiel ab, nachdem er seinen Nutzen für mich verloren hat.[/shake2]
intro2 = [shake2]Und rate mal, wer auch seinen Nutzen verloren hat. Richtig. Du.[/shake2]
intro3 = [shake2]Nun, da das besessene Plumpsklo mich nicht mehr belästigt, und alle deine Verträge abgeschlossen sind, brauche ich dich nicht mehr.[/shake2]
intro4 = [shake2]Achja, du hast nicht wirklich gedacht das du die [icon:timepiece][timepiece]Zeitstücke[/timepiece] behalten kannst, oder? Die sind in MEINEM Wald gelanded, Kleine! Sie gehören [scream]MIR[/scream].[/shake2]
intro6 = [shake2]Zeit dir zu zeigen was ich wirklich drauf habe, Kleine. Sag auf Wiedersehen zu deinem kleinen Kopf![/shake2]

Derp0 = So siehst du aus![br]Du bist ein Trottel!
Derp1 = Und du stinkst!

blue0 = [shake2]Du wartest bestimmt darauf, dass ich mich blau färbe, oder?[/shake2]
blue1 = [shake2]Sorry, Kleine. Blau steht mir nicht.[br]Das macht mich dann unbesiegbar, oder?[br]Ach, wie schade! [wave][scream]HA HA HA HA HA HA![/scream][/wave][/shake2]

Chemicals0 = [shake2]Mal schauen, wie dir [br]diese hier passen![/shake2]
Chemicals1 = [shake2]Nicht genug?[br]Wie wär's mit 1,000![/shake2]
