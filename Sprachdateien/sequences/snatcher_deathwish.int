[Hub]
deathwish_intro0 = [shake2]Netter Ort, den du hier hast, Kleine! Wie geht's? Noch am Leben und gesund, wie ich sehe. Welch eine Schande.[/shake2]
deathwish_intro1 = [shake2]Wie wäre es, wenn wir das mit einem kleinen Deal regeln? Ich schlag dir was vor; du erledigst ein paar neue Verträge für mich und ich werde dich gut belohnen. Wenn du überlebst.[/shake2]
deathwish_intro2 = [shake2]Ich habe diese [hint]kleine Karte[/hint] für dich. Du wirst feststellen, dass ich im Moment nur einen einzigen Vertrag darauf markiert habe, du musst ihn abschließen, bevor du den Rest freischaltest. Kontrollen und Überwachung, weißt du.[/shake2]
deathwish_intro3 = [shake2]Und sei vorsichtig, Kleine. Das sind nicht deine normalen, alltäglichen Verträge. Dies sind... [wave]fortgeschrittene Verträge.[/wave][/shake2]
deathwish_intro4 = [shake2]Wir wollen ja nicht, dass du bei der Arbeit [scream][big]STIRBST[/big][/scream], oder?[/shake2]
deathwish_intro5 = [shake][big][scream][wave]AHHHHAHAHAHAHAHAHAHA!![/wave][/scream][/big][/shake]

deathwish_post_intro0 = [shake2]Gut gemacht, Kleine! Du kannst eine Kerze anzünden, ich bin wirklich beeindruckt. Du kannst diese Verträge [hint]in jedem Akt abschließen[/hint]. Einige sind geeigneter als andere, also such dir einen aus![/shake2]
deathwish_post_intro1 = [shake2]Worauf wartest du noch? Na los! Husch![/shake2]

deathwish_coop_disabled = Snatcher kann leider immer nur einen Rechtskunden auf einmal nehmen.

deathwish_open0 = [shake2]Zurück für mehr, Kleine? Nun, lass uns einen Blick auf deine Karte werfen....[/shake2]
deathwish_open1 = [shake2]Nimm die Karte raus, Kleine. Wir machen ein paar Verträge.[/shake2]
deathwish_open2 = [shake2]Was willst du, Kleine? Ich lese gerade meinen Lieblingsteil dieses Buches erneut.[/shake2]
deathwish_open3 = [shake2]Bist du bereit, noch mehr zu sterben? Großartig![/shake2]
deathwish_open4 = [shake2]Hey, schau, es ist das nervige Mädchen, das die ganze Zeit stirbt![/shake2]
deathwish_open5 = [shake2]Hast du einen Todeswunsch, Kleine? Ich habe einige![/shake2]
deathwish_open6 = [shake2]Zeit, wieder an die Arbeit zu gehen![/shake2]
deathwish_open7 = [shake2]Zurück zum alten Schleifstein![/shake2]

dw_25_complete0 = [shake2]Wow Kleine, sieh an, wie du abgehst! Vielleicht bist du robuster, als du aussiehst.[/shake2]
dw_25_complete1 = [shake2]Du hast eine ganze Reihe von Verträgen hinter dir, aber es gibt noch viel mehr![/shake2]
dw_25_complete2 = [shake2]Also mach weiter, Kleine. Tick tack.[/shake2]

dw_50_complete0 = [shake2]Sieht so aus, als hättest du schon einige meiner Verträge abgeschlossen...[/shake2]
dw_50_complete1 = [shake2]Aber sei nicht zu stolz auf dich, ich bin erst halb fertig mit dir![/shake2]
dw_50_complete2 = [shake2]Es gibt noch viele Verträge zu erfüllen. Du dachtest nicht, dass du damit durchkommen könntest, nur die Hauptziele zu erreichen, oder?[/shake2]
dw_50_complete3 = [shake2]Jetzt mach schon, zurück an die Arbeit![/shake2]

dw_75_complete0 = [shake2]Du reißt geradezu durch diese Verträge, nicht wahr?[/shake2]
dw_75_complete1 = [shake2]Bei dieser Geschwindigkeit bist du im Handumdrehen damit fertig![/shake2]
dw_75_complete2 = [shake2]...Ich muss ganz ehrlich zu dir sein, Kleine. Ich hätte nicht erwartet, dass du noch so lange dieses irdische Mühsal mitmachst, weißt du?[/shake2]
dw_75_complete3 = [shake2]Aber hey, wer weiß? Dein nächster Tod könnte dein letzter sein![br]Wir könnten Glück haben![/shake2]
dw_75_complete4 = [shake2]Also dann, zurück an die Arbeit![/shake2]

dw_all_complete0 = [shake][scream]...WAS!? DU BIST NICHT TOT![/scream][/shake]
dw_all_complete1 = [shake2]Dieses Buch war nutzlos! Ich sollte klagen...[/shake2]
dw_all_complete2 = [shake2]Ugh. Du hast alle meine Verträge abgeschlossen, aber irgendwie bist du noch am Leben, um mich zu nerven. Danke, oder so.[/shake2]
dw_all_complete3 = [shake2]Wirklich. Das ist einfach großartig. Herzlichen Glückwunsch.[/shake2]
dw_all_complete4 = [shake2]...Huh? Willst du eine Belohnung? Hör zu, Kleine. Wir haben das schon einmal gemacht.[br]Du musst wirklich lernen, im Voraus zu verhandeln.[/shake2]
dw_all_complete5 = [shake2]Ich habe das Gefühl, dass du mich nicht in Ruhe lassen wirst, wenn ich dir nicht etwas übergebe. [br]Es geht immer um dich, was?[/shake2]
dw_all_complete6 = [shake2]Weißt du, du hättest es nicht einmal so weit schaffen sollen. Sie sollten gerade Blumen für dich aussuchen.[/shake2]
dw_all_complete7 = [shake2]Also schätze ich, wir können nicht immer bekommen, was wir wollen![/shake2]
dw_all_complete8 = [shake2]So, so ist ja gut. Mindestens hast du das Gefühl, dass du etwas erreicht hast. [br]Du hast es dir [wave]verdient[/wave].[/shake2]
dw_all_complete9 = [shake2]Wenn es nicht klar genug war, unser Geschäft ist abgeschlossen. Geh und belästige jemand anderen, Kleine.[/shake2]

deathwish_revisit0 = [shake2]Du schon wieder? Warum lässt du mich nicht in Ruhe?[/shake2]
deathwish_revisit1 = [shake2]Du weißt, dass es keine Verträge mehr gibt, oder?[/shake2]
deathwish_revisit2 = [shake2]Willst du ein paar alte Wunden wieder öffnen? Das geht für mich in Ordnung.[/shake2]
deathwish_revisit3 = [shake2]Erinnerst du dich, als du fast gestorben wärst? Ich möchte das noch einmal erleben.[/shake2]
deathwish_revisit4 = [shake2]Geh weg, Kleine, du störst mich.[/shake2]
deathwish_revisit5 = [shake2]Ich habe keine Arbeit mehr für dich, Kleine. Warum suchst du dir nicht einen richtigen Job?[/shake2]
deathwish_revisit6 = [shake2]Hast noch etwas zu erledigen, Kleine?[/shake2]